{
    "name": "pruga-vscode-preview",
    "displayName": "pruga-vscode-preview",
    "description": "preview builded web - for building use pruga-vscode-builder extension",
    "version": "0.0.1",
    "publisher": "domogled",
    "engines": {
        "vscode": "^1.5.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
       "workspaceContains:pruga.js"
    ],
    "main": "./out/src/extension",
    "contributes": {
        "commands": [{
            "command": "pruga.preview",
            "title": "pruga preview"
        }
        ]
    },
    "scripts": {
        "vscode:prepublish": "tsc -p ./",
        "compile": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}